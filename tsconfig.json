{
  "compilerOptions": {
    /* Strictness */
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitOverride": true,

    "baseUrl": ".",
    "esModuleInterop": true,
    "target": "ES2022",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2022"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "jsx": "preserve",
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "sourceMap": true,
    "isolatedModules": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@payload-config": [
        "./src/payload.config.ts"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "redirects.js",
    "next.config.js",
    "next-sitemap.config.cjs"
, "src/collections/Videos/components/VideoList.tsx.bak", "src/components/video/MuxUploaderWithList.tsx.bak", "src/collections/Videos/components/VideoAdmin.tsx.bak", "src/components/video/VideoAdmin.tsx.bak", "src/collections/Videos/components/DefaultListViewRefresher.tsx.bak", "src/components/EventBridge/index.tsx.bak", "src/services/events/EventService.ts.bak", "src/services/eventService.ts.bak", "src/services/events/eventEmitter.ts.bak"  ],
  "exclude": [
    "node_modules",
    "**/@payloadcms/storage-*/**/*"
  ]
}


